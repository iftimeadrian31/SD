<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Web Component App</display-name>
    <servlet>
        <servlet-name>ProcessBankOperation</servlet-name>
        <servlet-class>ProcessBankOperationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessStudent</servlet-name>
        <servlet-class>ProcessStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchStudentList</servlet-name>
        <servlet-class>FetchStudentListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStudent</servlet-name>
        <servlet-class>UpdateStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetIdUpdateStudent</servlet-name>
        <servlet-class>GetIdUpdateStudentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteStudent</servlet-name>
        <servlet-class>DeleteStudentServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ProcessCurs</servlet-name>
        <servlet-class>ProcessCursServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FetchCursList</servlet-name>
        <servlet-class>FetchCursListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateCurs</servlet-name>
        <servlet-class>UpdateCursServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetIdUpdateCurs</servlet-name>
        <servlet-class>GetIdUpdateCursServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCurs</servlet-name>
        <servlet-class>DeleteCursServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>ProcessBankOperation</servlet-name>
        <url-pattern>/process-bank-operation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessStudent</servlet-name>
        <url-pattern>/process-student</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FetchStudentList</servlet-name>
        <url-pattern>/fetch-student-list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStudent</servlet-name>
        <url-pattern>/update-student</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetIdUpdateStudent</servlet-name>
        <url-pattern>/get-id-update-student</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteStudent</servlet-name>
        <url-pattern>/delete-student</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessCurs</servlet-name>
        <url-pattern>/process-curs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FetchCursList</servlet-name>
        <url-pattern>/fetch-curs-list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateCurs</servlet-name>
        <url-pattern>/update-curs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetIdUpdateCurs</servlet-name>
        <url-pattern>/get-id-update-curs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCurs</servlet-name>
        <url-pattern>/delete-curs</url-pattern>
    </servlet-mapping>
</web-app>
